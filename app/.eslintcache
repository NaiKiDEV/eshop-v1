y {\r\n    yield put(logoutUserEnd());\r\n  } catch (e) {\r\n    // stops saga from breaking on api error\r\n  }\r\n}\r\n\r\nexport function* clearMessageSaga(action) {\r\n  try {\r\n    yield put(clearMessageEnd());\r\n  } catch (e) {\r\n    // stops saga from breaking on api error\r\n  }\r\n}\r\n\r\nexport default function* () {\r\n  yield takeLatest(GET_USER, getUserSaga);\r\n  yield takeLatest(REGISTER_USER, registerUserSaga);\r\n  yield takeLatest(LOGIN_USER, loginUserSaga);\r\n  yield takeLatest(LOGOUT_USER, logoutUserSaga);\r\n  yield debounce(5000, REGISTER_USER, clearMessageSaga);\r\n  yield debounce(2000, LOGIN_USER, clearMessageSaga);\r\n  yield debounce(2000, LOGOUT_USER, clearMessageSaga);\r\n}","C:\\Users\\Paulius\\Desktop\\eshop-v1\\eshop-v1\\app\\src\\store\\user\\reducer.js",[],"C:\\Users\\Paulius\\Desktop\\eshop-v1\\eshop-v1\\app\\src\\store\\user\\actions.js",[],"C:\\Users\\Paulius\\Desktop\\eshop-v1\\eshop-v1\\app\\src\\store\\user\\api.js",[],"C:\\Users\\Paulius\\Desktop\\eshop-v1\\eshop-v1\\app\\src\\store\\user\\actionTypes.js",[],"C:\\Users\\Paulius\\Desktop\\eshop-v1\\eshop-v1\\app\\src\\core\\httpClient.js",[],"C:\\Users\\Paulius\\Desktop\\eshop-v1\\eshop-v1\\app\\src\\store\\sagas.js",[],"C:\\Users\\Paulius\\Desktop\\eshop-v1\\eshop-v1\\app\\src\\pages\\register.jsx",[],"C:\\Users\\Paulius\\Desktop\\eshop-v1\\eshop-v1\\app\\src\\pages\\login.jsx",[],"C:\\Users\\Paulius\\Desktop\\eshop-v1\\eshop-v1\\app\\src\\store\\product\\sagas.js",["136"],"/* eslint-disable import/no-cycle */\r\n/* eslint-disable no-unused-vars */\r\nimport { takeLatest, call, put, debounce } from 'redux-saga/effects';\r\nimport {\r\n  getAllProductsApi,\r\n  addProductApi,\r\n  removeProductApi,\r\n  updateProductApi\r\n} from './api';\r\nimport {\r\n  GET_ALL_PRODUCTS,\r\n  ADD_PRODUCT,\r\n  ADD_TO_CART,\r\n  REMOVE_FROM_CART,\r\n  UPDATE_CART,\r\n  REMOVE_PRODUCT,\r\n  UPDATE_PRODUCT,\r\n  REMOVE_PRODUCT_END,\r\n  UPDATE_PRODUCT_END,\r\n} from './actionTypes';\r\nimp